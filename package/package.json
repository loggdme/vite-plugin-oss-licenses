{
  "name": "@loggd/vite-plugin-oss-licenses",
  "version": "1.1.0",
  "license": "MIT",
  "author": "loggd",
  "description": "A Vite plugin to generate a list of open source licenses used in your project.",
  "keywords": [
    "vite-plugin",
    "oss",
    "licenses",
    "open-source"
  ],
  "files": [
    "dist",
    "client.d.ts"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "bun run biome:check && bun run biome:lint && bun run biome:format",
    "upgrade": "bunx npm-check-updates --color -u",
    "biome:format": "biome format",
    "biome:lint": "biome lint",
    "biome:check": "biome check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loggdme/vite-plugin-oss-licenses.git",
    "directory": "packages/vite-plugin-oss-licenses"
  },
  "bugs": {
    "url": "https://github.com/loggdme/vite-plugin-oss-licenses/issues"
  },
  "homepage": "https://github.com/loggdme/vite-plugin-oss-licenses/blob/main/README.md",
  "dependencies": {
    "glob": "^13.0.0"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/node": "^24.10.1",
    "ultracite": "^6.3.8",
    "unbuild": "^3.6.1",
    "vite": "^7.2.4"
  }
}
